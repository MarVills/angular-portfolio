{
  "name": "angular-portfolio",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --proxy-config proxy.conf.json",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --configuration production",
    "test": "set NODE_OPTIONS=--openssl-legacy-provider && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --configuration production --base-href /angular-portfolio/ && copy dist\\angular-portfolio\\index.html dist\\angular-portfolio\\404.html && npx gh-pages -d dist/angular-portfolio"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.1",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.0.1",
    "@angular/compiler": "~11.0.1",
    "@angular/core": "~11.0.1",
    "@angular/forms": "~11.0.1",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "~11.0.1",
    "@angular/platform-browser-dynamic": "~11.0.1",
    "@angular/router": "~11.0.1",
    "@fortawesome/angular-fontawesome": "^0.8.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@ngx-translate/core": "^11.0.1",
    "@types/aos": "^3.0.3",
    "aos": "^2.3.4",
    "bootstrap-icons": "^1.13.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "resend": "^6.4.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "typed.js": "^2.0.11",
    "venobox": "^1.9.1",
    "websocket-driver": "0.7.4",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular/cli": "~11.0.2",
    "@angular/compiler-cli": "~11.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jquery": "^3.5.4",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^6.0.0",
    "gh-pages": "^6.3.0",
